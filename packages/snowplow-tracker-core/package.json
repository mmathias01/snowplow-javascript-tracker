{
  "name": "@snowplow/tracker-core",
  "version": "0.7.0",
  "devDependencies": {
    "@babel/cli": "^7.2.2",
    "mocha": "^5.2.0",
    "typescript": "3.2.2",
    "ts-node": "7.0.1",
    "nyc": "13.1.0",
    "source-map-support": "0.5.9"
  },
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "@types/uuid": "^2.0.29",
    "core-js": "^2.6.0",
    "lodash": "^4.17.11",
    "universal-base64url": "^1.0.0",
    "uuid": "3.3.2"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "BABEL_ENV=test nyc mocha --require ts-node/register --require @babel/register",
    "build": "npm run build:types && npm run build:js && npm run build:es",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "BABEL_ENV=cjs babel src --out-dir lib/cjs --extensions \".ts,.tsx\" --source-maps true",
    "build:es": "BABEL_ENV=mjs babel src --out-dir lib/mjs --extensions \".ts,.tsx\" --source-maps true",
    "prepublishOnly": "npm run build",
    "test:mocha": "BABEL_ENV=test mocha --require ts-node/register --require @babel/register --require source-map-support/register"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "rollup.config.js",
      ".babelrc.js",
      "**/lib/*",
      "**/test/*",
      "src/index.ts"
    ],
    "all": true  
  },
  "contributors": [
    "Alex Dean",
    "Simon Andersson",
    "Fred Blundun"
  ],
  "main": "./lib/cjs/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib/mjs/index.js",
  "description": "Core functionality for the Snowplow JavaScript trackers (browser JavaScript; Node.js; Segment.io server-side)",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowplow/snowplow-javascript-tracker.git"
  },
  "bugs": "https://github.com/snowplow/snowplow-javascript-tracker/issues",
  "keywords": [
    "tracking",
    "web analytics",
    "events",
    "open source"
  ],
  "license": "Apache-2.0"
}
