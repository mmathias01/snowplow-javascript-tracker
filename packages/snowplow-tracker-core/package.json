{
  "name": "@snowplow/tracker-core",
  "version": "0.7.0",
  "devDependencies": {
    "@babel/cli": "^7.2.2",
    "grunt": "^1.0.3",
    "intern": "3.3.2",
    "typescript": "^3.1.5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "@types/uuid": "^2.0.29",
    "core-js": "^2.6.0",
    "lodash": "^4.17.11",
    "universal-base64url": "^1.0.0",
    "uuid": "3.3.2"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "intern-client config=tests/intern",
    "build": "npm run build:types && npm run build:js && npm run build:es",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "BABEL_ENV=cjs babel src --out-dir lib/cjs --extensions \".ts,.tsx\" --source-maps inline",
    "build:es": "BABEL_ENV=mjs babel src --out-dir lib/mjs --extensions \".ts,.tsx\" --source-maps inline",
    "prepublishOnly": "npm run build"
  },
  "contributors": [
    "Alex Dean",
    "Simon Andersson",
    "Fred Blundun"
  ],
  "main": "./lib/cjs/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib/mjs/index.js",
  "description": "Core functionality for the Snowplow JavaScript trackers (browser JavaScript; Node.js; Segment.io server-side)",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowplow/snowplow-javascript-tracker.git"
  },
  "bugs": "https://github.com/snowplow/snowplow-javascript-tracker/issues",
  "keywords": [
    "tracking",
    "web analytics",
    "events",
    "open source"
  ],
  "license": "Apache-2.0"
}
