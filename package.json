{
  "name": "snowplow-tracker",
  "version": "2.11.0-ALPHA",
  "dependencies": {
    "browser-cookie-lite": "1.0.4",
    "core-js": "^2.6.0",
    "handlebars": "^4.0.12",
    "jstimezonedetect": "1.0.6",
    "murmurhash": "0.0.2",
    "randombytes": "^2.0.6",
    "rollup": "^0.67.3",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "sha1": "git://github.com/pvorb/node-sha1.git#910081c83f3661507d9d89e66e3f38d8b59d5559",
    "snowplow-tracker-core": "file:./core",
    "uglify-js": "^3.4.9",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.2.0",
    "babel-preset-minify": "^0.5.0",
    "JSON": "1.0.0",
    "eslint": "^5.9.0",
    "eslint-plugin-import": "^2.14.0",
    "grunt": "^1.0.3",
    "grunt-aws": "0.6.2",
    "intern": "3.3.2",
    "js-base64": "^2.4.9",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semver": "5.6.0"
  },
  "scripts": {
    "test:local": "npm run localtest.sh",
    "babel:tests:helpers": "babel dist/bundles/helpers.js -o tests/pages/helpers.js",
    "babel:tests:detectors": "babel dist/bundles/detectors.js -o tests/pages/detectors.js",
    "babel:main": "babel src --out-dir build --extensions \".js\" --source-maps inline --config-file=./babel-configs/.babelrc-main",
    "babel:second": "babel dist/bundles/snowplow.js -o dist/bundles/snowplow-second.js --config-file=./babel-configs/.babelrc-nobuiltins",
    "transpile": "npm-run-all babel:*",
    "browserify:tests:helpers": "browserify tests/scripts/helpers.js -o dist/bundles/helpers.js",
    "browserify:tests:detectors": "browserify tests/scripts/detectors.js -o dist/bundles/detectors.js",
    "browserify:tests:snowplow": "browserify build/js/init.js -o dist/bundles/test-bundle.js",
    "browserify:main": "browserify build/js/init.js -o dist/bundles/snowplow.js",
    "minify": "node scripts/minify.js",
    "template:integration": "node scripts/integration-template.js",
    "template:banner": "node scripts/banner.js",
    "templates": "npm-run-all template:*",
    "gzip:main": "gzip --best -c dist/min/sp.js > dist/zipped/sp.js",
    "concat:tag": "{ echo ';' ; cat dist/min/tag.js ; } > dist/tags.min.js",
    "concat:snowplow": "cat templates/filled/banner.txt dist/bundles/snowplow.js > dist/snowplow.js",
    "concat:sp": "cat templates/filled/banner.txt dist/min/sp.js > dist/sp.js",
    "concat": "npm-run-all concat:*",
    "tags": "minify:tags",
    "dirs": "mkdir -p dist/babel dist/bundles dist/min dist/zipped",
    "clean": "rm -rf dist/babel dist/bundles dist/min dist/zipped",
    "build:main": "npm-run-all clean dirs templates babel:main browserify:main babel:last minify concat gzip"
  },
  "contributors": [
    "Alex Dean",
    "Simon Andersson",
    "Anthon Pang",
    "Fred Blundun",
    "Joshua Beemster",
    "Michael Hadam"
  ],
  "description": "JavaScript tracker for Snowplow",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowplow/snowplow-javascript-tracker.git"
  },
  "bugs": "https://github.com/snowplow/snowplow-javascript-tracker/issues",
  "keywords": [
    "tracking",
    "web analytics",
    "events",
    "open source"
  ],
  "license": "Simplified BSD",
  "private": true,
  "browserslist": [
    "last 1 version",
    "> 2%",
    "not dead",
    "not ie <= 10"
  ]
}
